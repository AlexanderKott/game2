            motion:layout_constraintTop_toTopOf="@+id/tenetImage"
            motion:layout_editor_absoluteY="709dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteX="43dp" />
        <Constraint
            android:id="@+id/ground"
            android:layout_width="734dp"
            android:layout_height="647dp"
            motion:layout_constraintTop_toTopOf="@+id/tenetImage"
            motion:layout_editor_absoluteY="709dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteX="43dp" />
        <Constraint
            android:id="@+id/ground"
            android:layout_width="734dp"
            android:layout_height="647dp"
            motion:layout_constraintTop_toTopOf="@+id/tenetImage"
            motion:layout_editor_absoluteY="709dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteX="43dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/about"
            motion:layout_constraintStart_toStartOf="@+id/about"
            android:id="@+id/musicBtn" />
    </ConstraintSet>
</MotionScene>