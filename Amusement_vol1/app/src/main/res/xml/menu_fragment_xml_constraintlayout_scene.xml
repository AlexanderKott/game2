            motion:layout_editor_absoluteX="366dp"
            android:layout_width="85dp"
            motion:layout_constraintTop_toBottomOf="@+id/cats_game"
            motion:layout_editor_absoluteY="846dp" />
        <Constraint
            android:layout_width="85dp"
            android:layout_height="17dp"
            motion:layout_editor_absoluteX="366dp"
            motion:layout_constraintTop_toBottomOf="@+id/cats_game"
            motion:layout_editor_absoluteY="846dp" />
        <Constraint
            android:layout_height="300dp"
            motion:layout_editor_absoluteX="151dp"
            android:layout_width="300dp"
            motion:layout_constraintTop_toBottomOf="@+id/cats_game"
            android:id="@+id/ground" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            motion:layout_editor_absoluteY="476dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="84dp"
            android:layout_width="wrap_content" />
        <Constraint
            android:layout_height="392dp"
            android:layout_width="427dp"
            android:id="@+id/tenetImage"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="40dp"
            android:layout_marginLeft="40dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="1.0" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="56dp"
            motion:layout_constraintVertical_bias="0.060000002"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/cats_game"
            android:layout_marginTop="8dp"
            motion:layout_constraintHorizontal_bias="0.05" />
        <Constraint
